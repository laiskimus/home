## Home
Host home_agent
  HostName edge.lab.quidz.net
  Port 20522
  User quidz
  ForwardAgent yes

Host vadelma01
  Hostname vadelma01
  User pi
  ProxyJump home_agent

Host vadelma02
  Hostname vadelma02
  User pi
  ProxyJump home_agent
  
## Avanio
Host ajrlinux.ftp.sh
  HostName ajrlinux.ftp.sh
  Port 58362
  User msalmi
  ForwardAgent yes

Host nuc02.salodev.ajr.fi
  HostName nuc02.salodev.ajr.fi
  Port 58363
  User msalmi
  ForwardAgent yes

Host mikkos-admin
  Hostname mikkos-admin
  User msalmi
  ForwardAgent yes
  ServerAliveInterval    60
  TCPKeepAlive           yes
  ProxyJump nuc02.salodev.ajr.fi

Host mikkos-code
  Hostname mikkos-code
  User msalmi
  ForwardAgent yes
  ServerAliveInterval    60
  TCPKeepAlive           yes
  ProxyJump nuc02.salodev.ajr.fi

